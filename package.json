{
  "name": "elasticsearch-relay",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist src/generated/*",
    "start": "concurrently npm:watch-*",
    "watch-tsoa": "nodemon --watch src --watch tsoa.json --ext ts --ignore src/generated --exec 'tsoa spec-and-routes'",
    "watch-graphql": "graphql-codegen -w",
    "watch-schema": "nodemon --watch src/schema.graphql --exec \"npm run copy-schema\"",
    "watch-tsc": "tsc-watch --noClear --onSuccess \"node dist/server.js\"",
    "generate": "concurrently \"tsoa spec-and-routes\" \"graphql-codegen\"",
    "prebuild": "npm run generate",
    "build": "concurrently \"tsc\" \"npm:copy-schema\"",
    "docker-build": "docker build --tag elasticsearch-relay .",
    "copy-schema": "cpx src/schema.graphql dist"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.6.1",
    "apollo-server-express": "^2.13.1",
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-fields": "^2.0.3",
    "graphql-import": "^1.0.2",
    "graphql-tools": "^5.0.0",
    "swagger-ui-express": "^4.1.4",
    "tsoa": "^3.0.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.3",
    "@graphql-codegen/introspection": "1.13.3",
    "@graphql-codegen/typescript": "1.13.3",
    "@graphql-codegen/typescript-resolvers": "1.13.3",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/graphql": "^14.5.0",
    "@types/graphql-fields": "^1.3.2",
    "@types/node": "^13.11.1",
    "@types/swagger-ui-express": "^4.1.2",
    "concurrently": "^5.2.0",
    "cpx": "^1.5.0",
    "nodemon": "^2.0.3",
    "ts-node": "^8.10.1",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  }
}
